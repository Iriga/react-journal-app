ct(() => {\r\n        dispatch(activeNote(formValues.id, {...formValues}));\r\n    }, [formValues, dispatch])\r\n\r\n    const handleDelete = () => {\r\n        dispatch(startDeleting(id));\r\n    }\r\n\r\n\r\n    return (\r\n        <div className=\"notes__main-content\">\r\n            <NotesAppBar />\r\n\r\n            <div className=\"notes__content\">\r\n\r\n                <input\r\n                    type=\"text\"\r\n                    laceholder=\"Some awesome title\"\r\n                    className=\"notes__title-input\"\r\n                    autoComplete=\"off\"\r\n                    name=\"title\"\r\n                    value={title}\r\n                    onChange={handleInputChange}\r\n                />\r\n\r\n                <textarea\r\n                    placeholder=\"What happend today\"\r\n                    className=\"notes__textarea\"\r\n                    name=\"body\"\r\n                    value={body}\r\n                    onChange={handleInputChange}\r\n                >\r\n                </textarea>\r\n\r\n                {\r\n                    (note.url) &&\r\n                    <div className=\"notes__image\">\r\n                        <img src={note.url} alt=\"Image\"/>\r\n                    </div>\r\n                }\r\n\r\n            </div>\r\n\r\n            <button\r\n                className=\"btn btn-danger\"\r\n                onClick={handleDelete}\r\n            >\r\n                Delete\r\n            </button>\r\n\r\n        </div>\r\n    )\r\n}\r\n","D:\\Documentos\\React\\React De 0 a experto\\Sección 17 SASS Journal App\\journal-app\\src\\components\\journal\\JournalEntry.jsx",[],"D:\\Documentos\\React\\React De 0 a experto\\Sección 17 SASS Journal App\\journal-app\\src\\firebase\\firebase-config.js",[],"D:\\Documentos\\React\\React De 0 a experto\\Sección 17 SASS Journal App\\journal-app\\src\\actions\\auth.js",[],"D:\\Documentos\\React\\React De 0 a experto\\Sección 17 SASS Journal App\\journal-app\\